<% include ./partials/header %>

<link rel="stylesheet" href="/stylesheets/services.css">
<link rel="stylesheet" href="/stylesheets/contact.css">
<link rel="stylesheet" href="/stylesheets/mediaScreens.css">

<% include ./partials/navbar%>


    <div class="container">
        <div id="contact-subcontent" class="subcontent">
            <h1>Contact Us</h1>
            <div id="contacts-divider" class="ui horizontal divider"><i class="rocket icon"></i></div>
            <div class="form">
            <form action="/contact" method="POST">
                <div class="form-group">
                    <label for="name">Your Name</label>
                    <input class="form-control" type="text" name="contact[name]" placeholder="Enter Name">
                </div>
                <div class="form-group">
                    <label for="email">Your Email</label>
                    <input class="form-control" type="text" name="contact[email]" placeholder="Enter Email">
                </div>
                    <div id="textbox" class="form-group">
                        <label class="label">Your Message</label>
                    <textarea class="form-control" name="contact[message]" id="message" cols="30" rows="5" placeholder="Type Your Message Here"></textarea>
                </div>
                    <button class="ui dark button">Submit</button>
        </form>
        </div>  
    </div>
    </div>      
    
    


<% include ./partials/footer %>
</body>
</html>